          `select city, pop`.",

    "data": "zips.data",

    "query": "select distinct city from zips order by pop desc limit 5",

    "predicate": "equalsExactly",

    "expected": ["CHICAGO",
                 "BROOKLYN",
                 "NEW YORK",
                 "BELL GARDENS",
                 "LOS ANGELES"]
}
