{
    "name": "cities with largest individual zip codes",
    "backends": {
        "couchbase":  "skip",
        "marklogic_json": "pending",
        "marklogic_xml":  "pending",
        "mongodb_q_3_2": "pending",
        "postgresql": "pending",
        "spark_local": "skip",
